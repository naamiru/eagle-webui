    "@testing-library/react": "^16.3.0",
    "@types/etag": "^1.8.4",
    "@types/node": "^20",
    "@types/ospath": "^1.2.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "ajv": "^8.17.1",
    "etag": "^1.8.1",
    "jsdom": "^27.0.0",
    "media-chrome": "^4.14.0",
    "mime": "^4.1.0",
    "next": "15.5.4",
    "next-intl": "^4.3.12",
