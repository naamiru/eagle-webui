  },
  "license": "CC0-1.0",
  "author": "naamiru",
  "engines": {
    "node": ">=18.18.0"
  },
  "bin": {
    "eagle-webui": "start.js"
  },
  "files": [
    ".next/standalone",
    "start.js"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
